<div class='second-photo'>
  <div class="row firstname container">
    <h2>Sign up</h2>

    <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
      <%= devise_error_messages! %>

      <div class='field'>
        <div class="input-field col s12">
          <%= f.text_field :name, id: 'name', class: 'validate' %>
          <%= f.label :name, for: 'name' %><br />
        </div>
      </div>



    <div class='field'>
      <div class="input-field col s12">
        <%= f.text_field :ability, id: 'ability', class: 'validate' %>
        <%= f.label :ability, for: 'ability' %><br />
      </div>
    </div>


    <div class="field">
      <div class="input-field col s12">
        <%= f.email_field :email, id: 'email', class: 'validate', autofocus: true %>
        <%= f.label :email, for: 'ability' %><br />
      </div>
    </div>


    <div class="field">
      <div class="input-field col s12">
        <%= f.label :password, for: 'password' %>
        <%= f.password_field :password, id: 'password', class: 'validate', autocomplete: "off" %>
      </div>
    </div>



    <div class="field">
      <div class="input-field col s12">
        <%= f.label :password_confirmation %>
        <%= f.password_field :password_confirmation, autocomplete: "off" %>
      </div>
    </div>

      <div class="actions col s12 center sign">
        <%= f.submit "Sign up" %>
      </div>


    <% end %>

    <%= render "devise/shared/links" %>
  </div>
</div>
