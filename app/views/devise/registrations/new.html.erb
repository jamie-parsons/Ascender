<div class='second-photo'>
  <div class="row firstname">
    <h2>Sign up</h2>

    <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
      <%= devise_error_messages! %>

      <div class='field'>
        <div class="input-field col s6">
          <%= f.text_field :name, id: 'name', placeholder: 'name', class: 'validate' %>
          <%= f.label :name, for: 'name' %><br />
        </div>



    <div class='field'>
      <div class="input-field col s6">
        <%= f.text_field :ability, id: 'ability', placeholder: 'ability', class: 'validate' %>
        <%= f.label :ability, for: 'ability' %><br />
      </div>


    <div class="field">
      <div class="input-field col s6">
        <%= f.email_field :email, id: 'email', placeholder: 'email', class: 'validate' autofocus: true %>
        <%= f.label :email, for: 'ability' %><br />
      </div>

    <div class="field">
      <div class="input-field col s6">
        <%= f.label :password, for: 'password' %><br />
        <% if @minimum_password_length %>
        <em>(<%= @minimum_password_length %> characters minimum)</em>
        <% end %><br />
        <%= f.password_field :password, id: 'password', placeholder: 'password', class: 'validate' autocomplete: "off" %>
      </div>

    <div class="field">
      <div class="input-field col s6">
        <%= f.label :password_confirmation %><br />
        <%= f.password_field :password_confirmation, autocomplete: "off" %>
      </div>

      <div class="actions">
        <%= f.submit "Sign up" %>
      </div>
    <% end %>

    <%= render "devise/shared/links" %>
  </div>
</div>


